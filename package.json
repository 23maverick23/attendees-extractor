{
  "name": "attendees-extractor",
  "version": "0.1.0",
  "description": "Extracts attendee names from a markdown heading and updates the frontmatter property.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "attendees",
    "frontmatter"
  ],
  "author": "Ryan Morrissey",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "^0.15.0",
    "tslib": "2.3.1",
    "typescript": "^4.9.5"
  }
}
